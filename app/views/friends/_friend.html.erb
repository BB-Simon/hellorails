<tbody id="<%= dom_id friend %>">
  <tr>
    <td><%= friend.first_name %></td>
    <td><%= friend.last_name %></td>
    <td><%= friend.email %></td>
    <td><%= friend.phone %></td>
    <td><%= friend.twitter %></td>
    <td>
      <p>
        <%= link_to "Show this friend", friend %>
      </p>
    </td>
  </tr>
</tbody>
